    </Compile>
    <Compile Update="EventPlazaEditor.Designer.cs">
      <DependentUpon>EventPlazaEditor.cs</DependentUpon>
    </Compile>
    <Compile Update="EventPlazaTilesetEditor.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="EventPlazaTilesetEditor.Designer.cs">
      <DependentUpon>EventPlazaTilesetEditor.cs</DependentUpon>
    </Compile>
    <Compile Update="FolderSelectDialog\FolderSelectDialog.cs" />
