<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net48</TargetFramework>
    <OutputType>WinExe</OutputType>
    <UseWindowsForms>true</UseWindowsForms>
    <Platforms>x64</Platforms>
    <GenerateResourceUsePreserializedResources>true</GenerateResourceUsePreserializedResources>
  </PropertyGroup>
  <PropertyGroup>
    <ApplicationIcon>Images\icon.ico</ApplicationIcon>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="System" />
    <Reference Include="System.Core" />
    <Reference Include="System.Xml.Linq" />
    <Reference Include="System.Data.DataSetExtensions" />
    <Reference Include="Microsoft.CSharp" />
    <Reference Include="System.Data" />
    <Reference Include="System.Deployment" />
    <Reference Include="System.Drawing" />
    <Reference Include="System.Net.Http" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Xml" />
    <Reference Include="System.Resources.Extensions" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="ChannelClass.cs" />
    <Compile Update="EditChannel.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="EditChannel.Designer.cs">
      <DependentUpon>EditChannel.cs</DependentUpon>
    </Compile>
    <Compile Update="EventPlazaAnimationEditor.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="EventPlazaAnimationEditor.Designer.cs">
      <DependentUpon>EventPlazaAnimationEditor.cs</DependentUpon>
    </Compile>
    <Compile Update="EventPlazaCollisionEditor.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="EventPlazaCollisionEditor.Designer.cs">
